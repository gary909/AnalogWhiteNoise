<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.analogwhitenoise">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SeaActivity"
            android:label="@string/category_sea" />
        <activity
            android:name=".WindActivity"
            android:label="@string/category_wind" />
        <activity
            android:name=".FanActivity"
            android:label="@string/category_fan" />
        <activity
            android:name=".AirActivity"
            android:label="@string/category_air" />
        <activity android:name=".buyActivity" />
        <activity android:name=".infoActivity"></activity>
    </application>

</manifest>